    <Plugin>stockquote</Plugin>
    <MinVersion>1.0.0</MinVersion>
  </Dependency>
  <Dependency>
    <Plugin>rssutils</Plugin>
    <MinVersion>1.1.0</MinVersion>
  </Dependency>
<PluginType>STVI</PluginType>
 <Package>
   <PackageType>STVI</PackageType>
   <Location>https://github.com/OpenSageTV/sagetv-plugin-repo/releases/download/1/StockManager_2.1.1.zip</Location>
   <MD5>AE6EBFB473AA9A77DD1006E0C45E1AA4</MD5>
 </Package>
<STVImport>StockManager.stvi</STVImport>
